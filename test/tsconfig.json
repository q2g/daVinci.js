{
  "compileOnSave": true,
  "exclude": [
    ".vscode",
    "node_modules"
  ],
  "compilerOptions": {
    "module": "umd",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2015.iterable",
      "scripthost"
    ]
  }
}